{
  "name": "webviz_components",
  "version": "0.0.1",
  "description": "Webviz components for Plotly Dash",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "build:js-dev": "webpack --mode development",
    "build:js": "webpack --mode production",
    "build:py": "(. ../venv/bin/activate || ../venv\\scripts\\activate && dash-generate-components ./src/lib/components webviz_components)",
    "build:all": "npm run build:js && npm run build:js-dev && npm run build:py",
    "lint": "eslint \"./**/*.js\"",
    "lint:fix": "eslint --fix \"./**/*.js\"",
    "test": "jest",
    "test:update": "npm test -- -u",
    "test:watch": "npm run test -- --watch",
    "test:watchAll": "npm run test -- --watchAll",
    "analyze": "webpack --profile --json > stats.json"
  },
  "author": "Árni Geir Úlfarsson arnigeir@gmail.com",
  "license": "MIT",
  "dependencies": {
    "d3": "^5.7.0",
    "plotly": "^1.0.6",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.2.0",
    "styled-components": "3.4.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.9.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copyfiles": "^2.0.0",
    "css-loader": "^0.28.11",
    "deep-equal": "^1.0.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "github:webpack/file-loader",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "jest-transform-css": "^2.0.0",
    "jsverify": "^0.8.4",
    "npm": "^6.1.0",
    "postcss": "^7.0.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.3.0",
    "prettier-eslint": "^8.8.2",
    "react-docgen": "^2.20.1",
    "react-test-renderer": "^16.6.3",
    "sinon": "^7.1.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.1.2",
    "webpack": "4.19.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^2.1.3",
    "webpack-serve": "^1.0.2"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
